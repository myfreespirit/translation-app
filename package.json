{
  "name": "translation-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run express",
    "build-debug": "concurrently \"npm run express-debug\" \"npm run serve\"",
    "build-dev": "concurrently \"npm run express-dev\" \"npm run serve\"",
    "express-debug": "DEBUG=express:* DEBUG_COLORS=true nodemon --watch server server/index.js",
    "express-dev": "nodemon --watch server server/index.js",
    "express": "node server/index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@google-cloud/translate": "^5.3.0",
    "axios": "^0.19.2",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
